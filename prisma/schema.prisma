generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Category {
  NEW
  REVIEW
  FIRST_INTERVIEW
  SECOND_INTERVIEW
  DOCUMENT_PASS
  FINAL_PASS
  FAILURE
}

model Board {
  idx Int @id @default(autoincrement())
  category Category
  title String
  description String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  order Int
}